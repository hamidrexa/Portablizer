void delay (int i){
	while(i>0){
	TIM16->SR &=0xfe;
	  TIM16->CNT=0;
						while((!((TIM16->SR)&0x01)) );
			 
					 TIM16->CNT=0;
	i--;
	}
}
void wating(void){
	//GPIOA->ODR&=0x80;
GPIOA->ODR=0x30; 
		delay(100);
	while(1){
		 if( (GPIOB->IDR)&(0x02)){while((GPIOB->IDR)&(0x02));}
		 else{while(!((GPIOB->IDR)&(0x02)));break;}
		 
	 }
}
double num(int pa1,int pa2,int pa3,int pb){

		int a=0;
		int k[5];
		double x1;
		
		wating();
		delay(3);
				GPIOA->ODR=pa1; 
		delay(10);
		k[0]= (GPIOB->IDR)&(pb);
			a |= (k[0])<<5;//b d6 pa11
 
		 wating();
		delay(3);
			GPIOA->ODR=pa2; 
		delay(2);
		k[1]= (GPIOB->IDR)&(pb);
				a |= (k[1])<<6;//a	 d7 pa9
				
					 wating();
		delay(3);
					GPIOA->ODR=pa3; 
			delay(2);
		k[2]= (GPIOB->IDR)&(pb);
				a |= (k[2])<<1;//f	 d2 pa7
 
 
 
 //----------------------
 
		wating();
		delay(20);
				GPIOA->ODR=pa1; 
		delay(10);
		k[0]= (GPIOB->IDR)&(pb);
			a |= (k[0])<<4;//c d5 pa11
 
		 wating();
		delay(20);
			GPIOA->ODR=pa2; 
		delay(10);
		k[1]= (GPIOB->IDR)&(pb);
					a |= (k[1]) ;//g	 d1 pa9
				
					 wating();
		delay(20);
					GPIOA->ODR=pa3; 
			delay(10);
		k[2]= (GPIOB->IDR)&(pb);
	a |= (k[2])<<2;//e	 d3 pa7 
	//---------------------------
			wating();
		delay(55);
				GPIOA->ODR=pa1; 
		delay(10);
		k[0]= (GPIOB->IDR)&(pb);
	a |= (k[0])>>1;//p	d0 pa11

//	GPIOA->ODR|=0x80;
		 wating();
		GPIOA->ODR=pa2; 
		//	GPIOA->ODR&=0x7f;
		delay(65);
 
		
//		delay(10);
		k[1]= (GPIOB->IDR)&(pb);
						a |= (k[1])<<3;//d	 d4 pa9
		
 
 switch (a|0x01){
			case 0x03:{x1=0;break;}
			case 0x9f:{x1=1;break;}
			case 0x25:{x1=2;break;}
			case 0x0d:{x1=3;break;}
			case 0x99:{x1=4;break;}
			case 0x49:{x1=5;break;}
			case 0x41:{x1=6;break;}
			case 0x1b:{x1=7;break;}
			case 0x01:{x1=8;break;}
			case 0x09:{x1=9;break;}
		}
//	 		sprintf(buf,"a:%x ==x:%f\n",a ,x1);
// 				HAL_UART_Transmit(&huart1, (uint8_t*)buf, COUNTOF(buf)-1, 5000);
// 
				
		
			delay(100);
	 return x1;
}

