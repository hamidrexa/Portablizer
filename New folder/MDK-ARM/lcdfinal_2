void delay (int i){
	while(i>0){
	TIM16->SR &=0xfe;
	  TIM16->CNT=0;
						while((!((TIM16->SR)&0x01)) );
			 
					 TIM16->CNT=0;
	i--;
	}
}
void wating(void){
	//GPIOA->ODR&=0x80;
GPIOA->ODR=0x10; 
		delay(3);
	while(1){
		 if( (GPIOB->IDR)&(0x20)){while((GPIOB->IDR)&(0x20));}
		 else{while(!((GPIOB->IDR)&(0x20)));break;}
		 
	 }
}
double num (int pa1,int pa2,int pa3,int pb1,int pb2,int pb3,int *point,int duty ){

		int a=0;
		int k[5];
		double x1;
		char buf[50];
 
			wating();
				GPIOA->ODR=pa1; 
		delay(duty/6);
 if((GPIOB->IDR)&(pb1)) k[0]=1;else k[0]=0;
			a |= (k[0])<<6;//b d6 pa11

	GPIOA->ODR=pa2; 
		delay(2);
		if((GPIOB->IDR)&(pb2)) k[1]=1;else k[1]=0;
				a |= (k[1])<<7;//a	 d7 pa9
	
	
	GPIOA->ODR=pa3; 
			delay(2);
		if((GPIOB->IDR)&(pb3)) k[2]=1;else k[2]=0;
				a |= (k[2])<<2;//f	 d2 pa7
				
				
				//------------------------
			wating();
				GPIOA->ODR=pa1; 
		delay(duty/3);
	if((GPIOB->IDR)&(pb1)) k[0]=1;else k[0]=0;
			a |= (k[0])<<5;//c d5 pa11
 	
		GPIOA->ODR=pa2; 
	
	delay(2);
		if((GPIOB->IDR)&(pb2)) k[1]=1;else k[1]=0;
					a |= (k[1]<<1) ;//g	 d1 pa9	

GPIOA->ODR=pa3; 
 delay(2);
		if((GPIOB->IDR)&(pb3)) k[2]=1;else k[2]=0;
	a |= (k[2])<<3;//e	 d3 pa7 
 

	
	//-----------------------------------
	wating();
	GPIOA->ODR=pa1; 
			delay(duty*5/6);
 		if((GPIOB->IDR)&(pb1)) k[0]=1;else k[0]=0;
	a |= (k[0]) ;//p	d0 pa11
	
if(k[0])
*point=0;
else *point=1; 
GPIOA->ODR=pa2; 
			delay(2);

		if((GPIOB->IDR)&(pb2)) k[1]=1;else k[1]=0;
						a |= (k[1])<<4;//d	 d4 pa9

					 wating();
					
 

 //----------------------
 
	 
 
 switch (a|0x01){
			case 0x03:{x1=0;break;}
			case 0x9f:{x1=1;break;}
			case 0x25:{x1=2;break;}
			case 0x0d:{x1=3;break;}
			case 0x99:{x1=4;break;}
			case 0x49:{x1=5;break;}
			case 0x41:{x1=6;break;}
			case 0x1b:{x1=7;break;}
			case 0x01:{x1=8;break;}
			case 0x09:{x1=9;break;}
		}

//// 
////		
//		sprintf(buf,"iii:%x\n" ,a);
//HAL_UART_Transmit(&huart1, (uint8_t*)buf, COUNTOF(buf)-1, 5000);
//	
	//		delay(100);
	 return x1;
}

double read(void){
	double 	sum=0;
		int pa1,pa2,pa3,pb1,pb2,pb3;
	int i,j;
	double x1;
	int pointer,duty;
		// 7seg0
		//0_1**d3
		//0_2**d2
		//0_3**d1

		 		GPIOA->ODR=0x10; 
				wating();

i=0;
	while((GPIOB->IDR)&(0x20)){
		delay(1);
		i++;
		 
	 }
	duty=i;
pa1=0x30;
pa2=0x20;
pa3=0x10;
		
